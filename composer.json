{
    "name": "zablose/allog",
    "description": "Log all requests to the database.",
    "keywords": [
        "logging",
        "requests"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergejs Zablockis",
            "email": "zablose@gmail.com"
        }
    ],
    "require": {
        "php": "~7.4|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "zablose/dotenv": "^1.2.1"
    },
    "require-dev": {
        "facade/ignition": "^2.5.14",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^2.0.3",
        "fakerphp/faker": "^1.13.0",
        "guzzlehttp/guzzle": "^7.2.0",
        "laravel/framework": "^8.32.1",
        "laravel/tinker": "^2.6.1",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^5.3.0",
        "phpunit/phpunit": "^9.5.2"
    },
    "autoload": {
        "psr-4": {
            "Zablose\\Allog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "laravel/app/",
            "Tests\\": "laravel/tests/"
        },
        "classmap": [
            "laravel/database/seeders"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
