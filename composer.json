{
    "name": "zablose/allog",
    "description": "Log all requests to the database.",
    "keywords": [
        "logging",
        "requests"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergejs Zablockis",
            "email": "zablose@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "zablose/dotenv": "^2.0.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23.1",
        "laravel/framework": "^12.8.1",
        "laravel/tinker": "^2.10.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.0",
        "phing/phing": "^3.0.1",
        "phpunit/phpunit": "^11.5.17"
    },
    "autoload": {
        "psr-4": {
            "Zablose\\Allog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "laravel/app/",
            "Tests\\": "laravel/tests/",
            "Database\\Seeders\\": "laravel/database/seeders/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
